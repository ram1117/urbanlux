{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/merchandise/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/merchandise/tsconfig.app.json"
  },
  "projects": {
    "shared": {
      "type": "library",
      "root": "libs/shared",
      "entryFile": "index",
      "sourceRoot": "libs/shared/src",
      "compilerOptions": {
        "tsConfigPath": "libs/shared/tsconfig.lib.json"
      }
    },
    "merchandise": {
      "type": "application",
      "root": "apps/merchandise",
      "entryFile": "main",
      "sourceRoot": "apps/merchandise/src",
      "compilerOptions": {
        "tsConfigPath": "apps/merchandise/tsconfig.app.json"
      }
    },
    "admin": {
      "type": "application",
      "root": "apps/admin",
      "entryFile": "main",
      "sourceRoot": "apps/admin/src",
      "compilerOptions": {
        "tsConfigPath": "apps/admin/tsconfig.app.json"
      }
    },
    "authentication": {
      "type": "application",
      "root": "apps/authentication",
      "entryFile": "main",
      "sourceRoot": "apps/authentication/src",
      "compilerOptions": {
        "tsConfigPath": "apps/authentication/tsconfig.app.json"
      }
    },
    "ordering": {
      "type": "application",
      "root": "apps/ordering",
      "entryFile": "main",
      "sourceRoot": "apps/ordering/src",
      "compilerOptions": {
        "tsConfigPath": "apps/ordering/tsconfig.app.json"
      }
    }
  },
  "monorepo": true,
  "root": "apps/merchandise"
}
